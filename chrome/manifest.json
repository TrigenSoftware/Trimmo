{
    "manifest_version": 3,
    "name": "Trimmo",
    "version": "0.3.0",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["main.js"]
    }],
    "web_accessible_resources": [
        "trimmo.js"
    ],
    "permissions": [
        "tabs",
        "<all_urls>"
    ],
    "icons": {
        "128": "icon.png"
    }
}